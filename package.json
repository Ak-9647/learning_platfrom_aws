    "sync:tm": "node scripts/sync-issues-to-taskmaster.mjs"
  },
  "devDependencies": {
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "tsx": "^4.17.0"
  },
  "dependencies": {
    "zod": "^3.23.8"
  }
}
